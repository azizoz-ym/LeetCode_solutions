class Solution(object):
    def isPalindrome(self, x):
        if x < 0:
            return False
        else: 
            s = str(x)
            length = len(s)
            count = 0
            for i in range (0, length // 2):
                if (s[i] == s[length-1-i]):
                    count += 1
            if count == length // 2 :
                return True
            else:
                return False
                
        
